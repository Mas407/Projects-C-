        cout << "Correct Answer!" << endl;
        count++; 
    } else {
        cout << "Wrong Answer!\nCorrect Answer is Option :> 4\nObtained marks ::> " << count;
        break;
    }
    cout << "\n Predict the output:\nint a = 5;\ncout << (++a) + (a++);" << endl;
    cout << "1) 11\n2) 12\n3) 13\n4) Undefined Behavior" << endl;
    cin >> arr[i];
