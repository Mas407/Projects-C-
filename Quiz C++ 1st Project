        cout << "Wrong Answer!\nCorrect Answer is Option :> 3\nObtained marks ::> " << count;
        break;
    }
    cout << "\n What is the output of this code?\nchar *p = \"hello\";\np[0] = 'H';\ncout << p;" << endl;
    cout << "1) Hello\n2) hello\n3) Compilation error\n4) Segmentation fault" << endl;
    cin >> arr[i];
